{
  "schema_version": 1,
  "mixin": [
    "factoryapi.mixins.json"
  ],
  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": ["${mod_authors}"],
    "contact": {
      "sources": "${mod_source}"
    },
    "license": "${mod_license}",
    "icon": "icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "wily.factoryapi.quilt.FactoryAPIQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.2"
      },
      {
        "id": "architectury",
        "version": ">=6.5.69"
      }
    ]
  }
}