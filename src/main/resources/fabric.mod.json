{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contributors": [
    "${mod_credits}"
  ],
  "contact": {
    "sources": "${mod_source}",
    "issues": "${mod_issues}"
  },
  "license": "${mod_license}",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "wily.factoryapi.FactoryAPI::init"
    ],
    "client": [
      "wily.factoryapi.FactoryAPIClient::init"
    ],
    "modmenu": [
      "wily.factoryapi.base.compat.client.FactoryAPIModMenuCompat"
    ]
  },
  "accessWidener": "${aw}",
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabric-api": ">=${fabric_api_version}",
    "minecraft": ["26.1-alpha.1"]
  },
  "suggests": {
    "factocrafty": ">=0.0.0.6"
  }
}