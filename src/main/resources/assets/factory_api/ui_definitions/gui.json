{
  "targetUI": "gui",
  "applyCondition": "${windowHeight}>=720",
  "elements": {
    "big_text": {
      "type": "draw_string",
      "component": "A BIG TES(X)T",
      "translateX": "(${width}-${big_text.component.width}*${big_text.scaleX})/2",
      "translateY": "(${height}-7*${big_text.scaleY})/2",
      "color": "#FF00FF",
      "shadow": true,
      "scaleX": 2.5,
      "scaleY": 2.5
    },
    "diamond": {
      "type": "render_item",
      "item": {
        "common_item": "inventory_menu.slot.5"
      },
      "translateX": "(${width}-16*${diamond.scaleX})/2-80",
      "translateY": "(${height}-16*${diamond.scaleY})/2",
      "scaleX": 4,
      "scaleY": 4
    },
    "emerald": {
      "type": "render_item",
      "item": "emerald_block",
      "translateX": "(${width}-16*${emerald.scaleX})/2+160",
      "translateY": "(${height}-16*${emerald.scaleY})/2",
      "scaleX": 4,
      "scaleY": 4
    }
  }
}